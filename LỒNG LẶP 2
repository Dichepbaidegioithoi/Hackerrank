/* 5
12345     Ý TƯỞNG :i++ printf("%d",i);
23456              j=i+1;   printf("%d",i++)
34567
45678
56789
*/
#include <stdio.h>
#include <math.h>
int main()
{
	int n;
	scanf("%d",&n);
	for(int i=1;i<=n;i++)
	{
		printf("%d",i);
		for(int j=i+1;j<=i+n-1;j++)
		{
			printf("%d",j);
		}
		printf("\n");
	}
}
